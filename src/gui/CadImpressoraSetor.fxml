<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="340.0" prefWidth="504.0" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.CadImpressoraSetorController">
   <children>
      <Label text="Cadastro de Impressora por Setor">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <padding>
            <Insets left="5.0" top="5.0" />
         </padding>
      </Label>
      <HBox prefHeight="35.0" prefWidth="504.0">
         <children>
            <Label text="Setor">
               <padding>
                  <Insets left="5.0" top="10.0" />
               </padding>
            </Label>
            <Label text="Fabricante">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <padding>
                  <Insets left="93.0" top="10.0" />
               </padding>
            </Label>
            <Label text="Modelo">
               <padding>
                  <Insets left="65.0" top="10.0" />
               </padding>
            </Label>
         </children>
      </HBox>
      <HBox prefHeight="31.0" prefWidth="504.0">
         <children>
            <ComboBox fx:id="comboBoxSetor" prefHeight="25.0" prefWidth="115.0">
               <HBox.margin>
                  <Insets left="5.0" right="5.0" />
               </HBox.margin>
            </ComboBox>
            <ComboBox fx:id="comboBoxFabricante" prefHeight="25.0" prefWidth="115.0">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
            </ComboBox>
            <ComboBox fx:id="comboBoxModelo" prefWidth="115.0" />
            <Button fx:id="btSalvar" mnemonicParsing="false" onAction="#onBtSalvarAction" text="Salvar">
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </Button>
            <Label fx:id="lblConfirmacao" textFill="BLUE">
               <HBox.margin>
                  <Insets left="10.0" top="5.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label fx:id="lblErroSetor" textFill="RED">
               <padding>
                  <Insets left="5.0" />
               </padding>
            </Label>
            <Label fx:id="lblErroFabricante" textFill="RED">
               <padding>
                  <Insets left="93.0" />
               </padding>
            </Label>
            <Label fx:id="lblErroModelo" textFill="RED">
               <padding>
                  <Insets left="92.0" />
               </padding>
            </Label>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
