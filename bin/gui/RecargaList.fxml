<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="291.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.RecargaListController">
   <children>
      <Label text="Recarga de Impressoras">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <padding>
            <Insets left="5.0" top="5.0" />
         </padding>
      </Label>
      <ComboBox fx:id="comboBoxImpressora" onAction="#atualizaImpressoras" prefHeight="25.0" prefWidth="158.0" promptText="Escolha a impressora">
         <VBox.margin>
            <Insets left="5.0" top="5.0" />
         </VBox.margin>
      </ComboBox>
      <HBox prefHeight="21.0" prefWidth="600.0">
         <children>
            <Button fx:id="btRecarregar" mnemonicParsing="false" onAction="#onBtRecarregarAction" text="Recarregar">
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <TextArea fx:id="txtData" editable="false" prefHeight="200.0" prefWidth="200.0" />
   </children>
</VBox>
